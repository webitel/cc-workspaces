{
  "name": "cc-workspaces",
  "version": "22.12.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "build-and-run-locally": "npm run build && sudo docker run -p 80:80 -v ${PWD}/dist:/usr/share/nginx/html:rw -v ${HOME}/default.conf:/etc/nginx/conf.d/default.conf:ro -v ${PWD}/dist:/www/workspace:rw -v ${HOME}/Projects/web-client-auth/dist:/www/auth:ro --rm nginx",
    "link-local-webitel-cc-ui": "rm -rf node_modules/@webitel/cc-ui-sdk && ln -s /home/user/Projects/webitel-cc-ui-sdk node_modules/@webitel/cc-ui-sdk",
    "link-local-webitel-sdk": "rm -rf node_modules/webitel-sdk && ln -s /home/user/Projects/webitel_sdk/dist node_modules/webitel-sdk",
    "lint": "vue-cli-service lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^3.2.8",
    "@webitel/cc-ui-sdk": "^0.1.27-3",
    "@webitel/ui-sdk": "^2.1.40-1",
    "axios": "^0.27.2",
    "core-js": "^3.6.4",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.0.3",
    "deepmerge": "^4.2.2",
    "emoji-picker-element": "^1.10.1",
    "file-saver": "^2.0.5",
    "github-markdown-css": "^4.0.0",
    "insert-text-at-cursor": "^0.3.0",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "linkify-html": "^3.0.5",
    "linkifyjs": "^3.0.5",
    "markdown-it": "^12.3.2",
    "tinymce": "^6.3.1",
    "vue": "^2.6.14",
    "vue-i18n": "^8.15.3",
    "vue-router": "^3.5.3",
    "vuelidate": "^0.7.5",
    "vuex": "^3.6.2",
    "webitel-sdk": "^0.1.175"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-router": "^5.0.4",
    "@vue/cli-plugin-unit-jest": "^5.0.4",
    "@vue/cli-plugin-vuex": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0-alpha.4",
    "bamboo-jest-reporter": "^1.0.3",
    "eslint": "^8.14.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.0",
    "jsdom": "^16.4.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "svg-sprite-loader": "^6.0.11",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^4.6.1"
  }
}
