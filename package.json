{
  "name": "cc-workspaces",
  "version": "20.12.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --hot",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "build-and-run-locally": "npm run build && sudo docker run -p 80:80 -v ${PWD}/dist:/usr/share/nginx/html:rw -v ${HOME}/default.conf:/etc/nginx/conf.d/default.conf:ro -v ${PWD}/dist:/www/workspace:rw -v ${HOME}/Projects/web-client-auth/dist:/www/auth:ro --rm nginx",
    "link-local-webitel-cc-ui": "rm -rf node_modules/@webitel/cc-ui-sdk && ln -s /home/user/Projects/webitel-cc-ui-sdk node_modules/@webitel/cc-ui-sdk",
    "link-local-webitel-sdk": "rm -rf node_modules/webitel-sdk && ln -s /home/user/Projects/webitel_sdk/dist node_modules/webitel-sdk",
    "lint": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@webitel/cc-ui-sdk": "^0.1.16",
    "@webitel/ui-sdk": "^0.8.42",
    "axios": "^0.19.2",
    "bamboo-jest-reporter": "^1.0.3",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.0.3",
    "file-loader": "^5.0.2",
    "github-markdown-css": "^4.0.0",
    "jsdom": "^16.4.0",
    "linkifyjs": "^3.0.0-beta.3",
    "markdown-it": "^10.0.0",
    "moment": "^2.24.0",
    "svg-sprite-loader": "^5.0.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.3",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.1.5",
    "vuejs-datepicker": "^1.6.2",
    "vuelidate": "^0.7.5",
    "vuex": "^3.1.2",
    "webitel-sdk": "^0.1.73"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.1.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
