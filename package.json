{
  "name": "cc-workspaces",
  "version": "23.12.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "build-and-run-locally": "npm run build && sudo docker run -p 80:80 -v ${PWD}/dist:/usr/share/nginx/html:rw -v ${HOME}/default.conf:/etc/nginx/conf.d/default.conf:ro -v ${PWD}/dist:/www/workspace:rw -v ${HOME}/Projects/web-client-auth/dist:/www/auth:ro --rm nginx",
    "link-local-webitel-cc-ui": "rm -rf node_modules/@webitel/cc-ui-sdk && ln -s /home/user/Projects/webitel-cc-ui-sdk node_modules/@webitel/cc-ui-sdk",
    "link-local-webitel-sdk": "rm -rf node_modules/webitel-sdk && ln -s /home/user/Projects/webitel_sdk/dist node_modules/webitel-sdk",
    "lint": "vue-cli-service lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^3.2.8",
    "@vue/compat": "^3.3.9",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.4.1",
    "@webitel/ui-sdk": "^24.2.9",
    "autolinker": "^4.0.0",
    "axios": "^1.6.5",
    "core-js": "^3.6.4",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.0.3",
    "deepmerge": "^4.2.2",
    "emoji-picker-element": "^1.10.1",
    "file-saver": "^2.0.5",
    "github-markdown-css": "^4.0.0",
    "insert-text-at-cursor": "^0.3.0",
    "is-electron": "^2.2.2",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "markdown-it": "^12.3.2",
    "prepend-http": "^4.0.0",
    "tinymce": "^6.3.1",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "webitel-sdk": "^23.12.13"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-plugin-vuex": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/vue3-jest": "^29.2.2",
    "babel-preset-vite": "^1.1.0",
    "bamboo-jest-reporter": "^1.0.3",
    "eslint": "^8.14.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.0",
    "jest-environment-jsdom": "^29.4.3",
    "jsdom": "^16.4.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "svg-sprite-loader": "^6.0.11",
    "webpack-bundle-analyzer": "^4.6.1"
